<?xml version="1.0" encoding="utf-8" ?>
<odoo>
  <!-- l10n_latam.identification.type tree inherit view -->
  <record id="view_l10n_latam_identification_type_tree_inherit" model="ir.ui.view">
    <field name="name">l10n_latam.identification.type.view.tree.inherit</field>
    <field name="model">l10n_latam.identification.type</field>
    <field
      name="inherit_id"
      ref="l10n_latam_base.view_l10n_latam_identification_type_tree"
    />
    <field name="arch" type="xml">
      <xpath expr="//field[@name='active']" position="before">
        <field name="l10n_ec_tpidprov" />
        <field name="l10n_ec_tpidcliente" />
        <field name="l10n_ec_tipoidinformante" />
        <field name="l10n_ec_tpidclienteex" />
      </xpath>
    </field>
  </record>
</odoo>
